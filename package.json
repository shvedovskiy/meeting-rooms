{
  "name": "meeting-rooms",
  "version": "0.0.1",
  "description": "Meeting Rooms App",
  "author": "Oleg Shvedov <ol.shvedov@gmail.com> (https://shvedovskiy.github.io/)",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "github:shvedovskiy/meeting-rooms",
  "bugs": {
    "url": "https://github.com/shvedovskiy/meeting-rooms/issues",
    "email": "ol.shvedov@gmail.com"
  },
  "main": "./dist/index.js",
  "proxy": "http://localhost:4000",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "dev:client": "cross-env BROWSER=none react-scripts start",
    "build:client": "react-scripts build && node scripts/insert-preloads.js && precompress -e .DS_Store,.map ./build/",
    "test": "react-scripts test",
    "dev:server": "ts-node-dev --respawn --debug --project server/tsconfig.json server/index.ts",
    "build:server": "tsc -p server && node ./dist/db-setup.js",
    "test:server": "cross-env NODE_ENV=test jest --config=./server/jest.config.js --forceExit --detectOpenHandles",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "run-s build:*",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "lint:typescript": "tsc -p server/tsconfig.json --noEmit && tsc --noEmit",
    "lint:eslint": "eslint '{src,server}/**/*.{js?(x),ts?(x)}' --fix",
    "lint:prettier": "prettier --write \"{src,server}/**/*.{js?(x),ts?(x),css,json}\"",
    "lint:stylelint": "stylelint \"src/**/*.css\" \"public/**/*.html\"",
    "lint": "run-s lint:*",
    "db-setup": "cross-env ts-node --project ./server/tsconfig.json ./server/db-setup.ts"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.13",
    "class-validator": "^0.11.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "date-fns": "^2.8.1",
    "dotenv-flow": "^3.1.0",
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.5",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "npm-run-all": "^4.1.5",
    "reflect-metadata": "^0.1.13",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^4.1.1",
    "tslib": "^1.10.0",
    "type-graphql": "^0.17.5",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.21",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@react-hook/throttle": "^1.0.9",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/classnames": "^2.2.9",
    "@types/dotenv-flow": "^3.0.0",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.8",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.15",
    "@types/react-aria-modal": "^4.0.1",
    "@types/react-dom": "^16.9.4",
    "@types/react-select": "^3.0.8",
    "@types/react-transition-group": "^4.2.3",
    "@types/serve-favicon": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@use-it/event-listener": "^0.1.3",
    "a11y-react-emoji": "^1.1.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "classnames": "^2.2.6",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "husky": "^3.1.0",
    "jest-extended": "^0.11.2",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "pluralize-ru": "^1.0.1",
    "precompress": "^5.1.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-aria-modal": "^4.0.0",
    "react-day-picker": "^7.4.0",
    "react-dom": "^16.12.0",
    "react-error-boundary": "^1.2.5",
    "react-scripts": "^3.3.0",
    "react-select": "^3.0.8",
    "react-tiny-popover": "^4.0.0",
    "react-transition-group": "^4.3.0",
    "stylelint": "^12.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.13.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "update-input-width": "^1.1.1",
    "use-media": "^1.4.0",
    "utility-types": "^3.10.0"
  }
}
