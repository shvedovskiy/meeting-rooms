{
  "name": "meeting-rooms",
  "version": "0.0.1",
  "description": "Meeting Rooms App",
  "author": "Oleg Shvedov <ol.shvedov@gmail.com> (https://shvedovskiy.github.io/)",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "github:shvedovskiy/meeting-rooms",
  "bugs": {
    "url": "https://github.com/shvedovskiy/meeting-rooms/issues",
    "email": "ol.shvedov@gmail.com"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "start:all": "concurrently \"npm run server:dev\" \"npm start\"",
    "build:all": "run-s build server:build",
    "lint:typescript": "tsc -p server/tsconfig.json --noEmit && tsc --noEmit",
    "lint:eslint": "eslint '{src,server}/**/*.{js,jsx,ts,tsx}' --fix",
    "lint:prettier": "prettier --write \"{src,server}/**/*.{js,jsx,ts,tsx,css,json}\"",
    "lint:stylelint": "stylelint \"src/**/*.css\" \"public/**/*.html\"",
    "lint": "run-s lint:*",
    "server:dev": "cross-env NODE_ENV=development ts-node-dev --respawn --debug --project server/tsconfig.json server/index.ts",
    "server:build": "cross-env NODE_ENV=production REFRESH_DB=true tsc -p server && node ./dist/db-setup.js",
    "server:run": "node dist/index.js",
    "db-setup": "cross-env REFRESH_DB=true ts-node --project ./server/tsconfig.json ./server/db-setup.ts",
    "server:test": "cross-env NODE_ENV=test jest --config=./server/jest.config.js --forceExit --detectOpenHandles"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@react-hook/throttle": "^1.0.5",
    "@use-it/event-listener": "^0.1.3",
    "a11y-react-emoji": "^1.1.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-server-express": "^2.9.9",
    "class-validator": "^0.11.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "date-fns": "^2.7.0",
    "dotenv-flow": "^3.1.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "node-sass": "^4.13.0",
    "pluralize-ru": "^1.0.1",
    "react": "^16.11.0",
    "react-aria-modal": "^4.0.0",
    "react-day-picker": "^7.4.0",
    "react-dom": "^16.11.0",
    "react-scripts": "^3.1.1",
    "react-select": "^3.0.8",
    "react-transition-group": "^4.3.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.0",
    "tslib": "^1.10.0",
    "type-graphql": "^0.17.5",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.20",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^3.7.2",
    "update-input-width": "^1.1.1",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/dotenv-flow": "^3.0.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.7",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-aria-modal": "^4.0.1",
    "@types/react-dom": "^16.9.4",
    "@types/react-select": "^3.0.8",
    "@types/react-transition-group": "^4.2.3",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "husky": "^3.0.9",
    "jest-extended": "^0.11.2",
    "lint-staged": "^9.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.12.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.0",
    "ts-node-dev": "^1.0.0-pre.41",
    "utility-types": "^3.10.0"
  }
}
