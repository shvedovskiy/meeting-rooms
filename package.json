  "scripts": {
    "start": "concurrently \"npm run server:start\" \"react-scripts start\"",
    "server:start": "nodemon ./server/index.js",
    "test-server:start": "NODE_ENV=test npm run reset-db && NODE_ENV=test npm run server:start",
    "build": "react-scripts build",
    "reset-db": "node ./server/create-mock-data.js",
    "test": "react-scripts test",
    "test:server": "npx jest",
    "lint:eslint": "eslint src server --ext .js,.jsx --fix",
    "lint:prettier": "prettier --write \"{src,server}/**/*.{js,jsx,css,json}\"",
    "lint:stylelint": "stylelint \"src/**/*.css server/**/*.css public/**/*.html\"",
    "lint": "run-s lint:* && flow",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "dependencies": {
    "apollo-server-express": "^2.4.6",
    "dotenv-flow": "^0.2.0",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "graphql-date": "^1.0.3",
    "graphql-tools": "^4.0.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.0",
    "redux-starter-kit": "^0.4.2",
    "reselect": "^4.0.0",
    "sequelize": "^4.42.0",
    "serve": "^10.1.1",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-react-app": "^3.0.7",
    "flow-bin": "^0.91.0",
    "graphql-tester": "0.0.5",
    "husky": "^1.3.1",
    "invariant": "^2.2.4",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1",
    "redux-mock-store": "^1.5.3",
    "run-s": "0.0.0",
    "source-map-explorer": "^1.6.0",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.0.0"
  }
}
