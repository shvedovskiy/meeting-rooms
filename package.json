{
  "name": "meeting-rooms",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"nodemon ./server/index.js\" \"react-scripts start\"",
    "build": "react-scripts build",
    "test": "npm run reset-db && react-scripts test",
    "lint:eslint": "eslint src server --ext .js,.jsx --fix",
    "lint:prettier": "prettier --write \"{src,server}/**/*.{js,jsx,css,json}\"",
    "lint:stylelint": "stylelint \"src/**/*.css server/**/*.css public/**/*.html\"",
    "lint": "run-s lint:* && flow",
    "analyze": "source-map-explorer build/static/js/main.*",
    "reset-db": "rm -f ./server/db.sqlite3 && node ./server/create-mock-data.js",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.1.1",
    "graphql-date": "^1.0.3",
    "graphql-tools": "^4.0.4",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.0",
    "redux-starter-kit": "^0.4.2",
    "reselect": "^4.0.0",
    "sequelize": "^4.42.0",
    "serve": "^10.1.1",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "flow-bin": "^0.91.0",
    "graphql-tester": "0.0.5",
    "husky": "^1.3.1",
    "invariant": "^2.2.4",
    "jest-enzyme": "^7.0.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1",
    "react-test-renderer": "^16.7.0",
    "redux-mock-store": "^1.5.3",
    "run-s": "0.0.0",
    "source-map-explorer": "^1.6.0",
    "stylelint": "^9.10.1"
  }
}
